@model Cinema.Movie.Common.DashboardPageModel

@section Head {

}

<div class="col-md-2 hidden-xs">
    <div class="box box-primary">
        <div class="box-header ">
            <b class="uppercase header">@LocalText.Get("Db.Movie.Genre.EntityPlural")</b>
        </div>
        <div class="box-body">
            <ul class="list-group list-group-unbordered">
                @foreach (var i in Model.Genres)
                {
                    <li class="list-group-item">
                         <a href="/genre/@i.GenreId"><b>@i.Name</b></a>
                    </li>
                }
            </ul>
        </div>
        <!-- /.box-body -->
    </div>
</div>
<div class="col-md-8 col-xs-12">
    <div class="box box-primary">
        <div class="box-header uppercase">
            <b>  @ViewData["Title"]</b>
        </div>
        <div class="box-body box-profile">
        </div>
        <!-- /.box-body -->
    </div>
</div>

<div class="col-md-2 hidden-xs">
    <div class="box box-primary">
        <div class="box-header">
            
        </div>
        <div class="box-body box-profile">
        </div>
        <!-- /.box-body -->
    </div>
</div>