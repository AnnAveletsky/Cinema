@model List<Cinema.Movie.Movie.Entities.GenreRow>

<div class="box box-primary">
    <div class="box-header ">
        <b class="uppercase header">@LocalText.Get("Db.Movie.Genre.EntityPlural")</b>
    </div>
    <div class="box-body">
        <ul class="list-group list-group-unbordered">
            @foreach (var i in Model)
            {
                <li class="list-group-item">
                    <b><a href="~/?genre=@i.Name">@i.Name</a></b>
                </li>
            }
        </ul>
    </div>
</div>
